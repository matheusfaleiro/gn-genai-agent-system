# API Authentication (required - server will not accept requests without this)
API_KEY=your-secret-api-key

# Azure OpenAI Configuration
# AZURE_OPENAI_ENDPOINT: Your resource endpoint (no trailing slash)
# AZURE_OPENAI_DEPLOYMENT: The deployment name you created in Azure (not the model name)
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_DEPLOYMENT=your-deployment-name
AZURE_API_VERSION=2024-12-01-preview

# Agent Configuration
API_BASE_URL=http://localhost:8000/v1
LOG_LEVEL=WARNING
